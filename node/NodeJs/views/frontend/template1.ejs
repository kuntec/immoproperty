<% if(data){ %>
<section class="no-padding-tb sm-padding-60px-top overflow-visible bg-extra-light-gray xs-padding-60px-tb">
	<div class="container">
		<div class="row vertical-center">
			<div class="col-md-6 col-sm-12 text-left sm-text-center">
				<h1 class="text-55 xs-text-28 font-weight-800 text-black margin-10px-bottom">	<%= data.name %></h1>
				<p class="text-22 xs-text-22 font-weight-300 text-dark-gray no-margin-bottom">	<%= data.description %> </p>
			</div>
			<div class="col-md-6 col-sm-12 xs-display-none">
				<div class="bg-img" style="background: #fff url('/images/<%- data.photos[0] %>'); background-position: center; height: 600px; width: 85%; background-size: cover; border-radius: 0 30%; box-shadow : -4px 4px 8px 0px rgba(0,0,0,0.5); margin: 40px auto -40px;" ></div>
				<!--<img src="image/house.jpg">-->
			</div>
    	</div>
  	</div>
</section>
<section class="no-padding-bottom">
	<div class="container">
		<div class="row">
			<div class="col-md-6">
				<h2 class="text-22 text-extra-dark-gray no-margin-bottom"><%= data.name %></h2>
				<p class="text-16 text-dark-gray"><%= data.description %></p>
			</div>
			
			
			<div class="col-md-6">
					<h6 ><%- translation.DOWNLOAD_DOC %></h6>
					
					<% if(data.documents.length > 0){ %>
					
					<% for(var t=0; t<data.documents.length;t++){  %>
						
						
							<a href="/documents/<%- data.documents[t] %>" target="_blank">
							 <img src="/common/file_image.png" height="50px" width="50px" />
							</a>						 
						 
					 <% } %>
					 
					<% } else { %>
						<strong><%- translation.NO_DOC_FOUND %> </strong>
					<% }  %>
			</div>
		</div>
	</div>	
</section>
